# Copy this file to .env.local and fill in your contract details

# Stellar Testnet RPC URL (default is fine)
NEXT_PUBLIC_RPC_URL=https://soroban-testnet.stellar.org

# Your deployed contract ID (get this after deployment)
# Example: CACDYF3CYMJEJTIVFESQYZTN67GO2R5D5IUABTCUG3HXQSRXCSOROBAN
NEXT_PUBLIC_CONTRACT_ID=YOUR_CONTRACT_ID_HERE

# Network passphrase for testnet
NEXT_PUBLIC_NETWORK_PASSPHRASE=Test SDF Network ; September 2015
